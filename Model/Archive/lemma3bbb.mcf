[true*] forall replica1, leader1: Pos, msg1: Message .
val(type(msg1) == preCommit && replica1 != 4 && leader1 != 4) =>  [message_sent(msg1, replica1, leader1)] .
[true*] forall replica2, leader2: Pos, msg2: Message .
val(type(msg2) == preCommit && replica2 != 4 && replica2 != replica1 &&
viewNumber(msg2) == viewNumber(msg1) && node(msg2) == node(msg1)) =>  
[message_sent(msg2, replica2, leader2)] .
[true*] exists replica3, leader3: Pos, msg3: Message . 
val(type(msg3) == commit && replica3 != 4 && viewNumber(msg3) == viewNumber(msg1) &&
node(msg3) == node(msg1)) => 
[message_sent(msg3, replica3, leader3)]