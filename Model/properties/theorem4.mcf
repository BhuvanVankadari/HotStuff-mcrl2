[true*] forall leader: Pos, phase: MessageType, view: Nat .
(
    <phase_change_leader(leader, phase, view)>true &&
    val(phase == prepare) &&
    <timeout(view)>false
) =>
(
    <execute_command(COMMAND_LIST.view, view) . true* . execute_command(COMMAND_LIST.view, view) . true* . 
    execute_command(COMMAND_LIST.view, view)>true
)